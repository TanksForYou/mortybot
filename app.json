{
  "name": "Mortybot",
  "description": "Morty is the man.",
  "repository": "https://github.com/TanksForYou/mortybot.git",
  "success_url": "/",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "MORTYBOT_COMMAND_TOKEN": {
      "description": "Slash command token, for the mortybot command endpoint",
      "required": true
    },
    "SLACK_TOKEN": {
      "description": "Slack bot RTM API token",
      "required": false
    },
    "NODE_ENV": "staging"
  },
  "image": "heroku/nodejs"
}
